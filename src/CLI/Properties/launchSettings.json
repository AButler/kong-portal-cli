    },
    "CLI - Sync (Apply)": {
      "commandName": "Project",
      "commandLineArgs": "sync --input c:\\temp\\kong --apply --konnect-token-file c:\\temp\\kong.token --konnect-addr https://eu.api.konghq.com"
